{
  "name": "@v1b3x0r/mds-core",
  "version": "5.0.0",
  "description": "Living World Simulation Engine. JSON entities that think, feel, learn, and form societies.",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/mds-core.esm.js",
  "module": "./dist/mds-core.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/mds-core.esm.js"
    }
  },
  "files": [
    "dist",
    "materials",
    "examples",
    "docs",
    "README.npm.md",
    "LICENSE"
  ],
  "readme": "README.npm.md",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build && test -f README.github.md || mv README.md README.github.md && cp README.npm.md README.md",
    "postpublish": "test -f README.github.md && mv README.github.md README.md || true"
  },
  "keywords": [
    "intention-engine",
    "meaning-simulation",
    "living-materials",
    "autonomous-behavior",
    "emergent-systems",
    "essence-first",
    "semantic-computing",
    "entity-component",
    "simulation",
    "info-physics",
    "hci",
    "mds",
    "game-engine"
  ],
  "author": "v1b3x0r",
  "license": "MIT",
  "homepage": "https://github.com/v1b3x0r/mds#readme",
  "repository": {
    "type": "git",
  "url": "git+https://github.com/v1b3x0r/mds.git"
  },
  "bugs": {
  "url": "https://github.com/v1b3x0r/mds/issues"
  },
  "devDependencies": {
    "terser": "^5.44.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
