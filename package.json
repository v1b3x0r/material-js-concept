{
  "name": "@v1b3x0r/mds-core",
  "version": "3.0.0",
  "description": "Material Definition System - Think in materials, not CSS properties",
  "sideEffects": false,
  "main": "./dist/mds.umd.js",
  "module": "./dist/mds.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/mds.esm.js",
      "require": "./dist/mds.umd.js"
    }
  },
  "files": [
    "dist",
    "manifests",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ui",
    "design-system",
    "material",
    "animation",
    "interface",
    "glassmorphism",
    "interactive",
    "motion",
    "physics",
    "soft-ui",
    "mds",
    "hci"
  ],
  "author": "v1b3x0r",
  "license": "MIT",
  "homepage": "https://github.com/v1b3x0r/material-js-concept#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v1b3x0r/material-js-concept.git"
  },
  "bugs": {
    "url": "https://github.com/v1b3x0r/material-js-concept/issues"
  },
  "devDependencies": {
    "terser": "^5.44.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
